             xmlns:local="clr-namespace:Celic"
             StartupUri="UI/Windows/Main/MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source="UI/CustomControls/Themes/Generic.xaml"/>
                <ResourceDictionary Source="UI/CustomControls/Themes/CustomControlsTemplate.xaml"/>
